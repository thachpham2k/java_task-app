@Data
@NoArgsConstructor
@AllArgsConstructor
@Entity
@Table(name="role")

@Id
@GeneratedValue(strategy = GenerationType.IDENTITY)
@Column(name="id")

@OneToMany(mappedBy = "project")
private List<ProjectManage> projectManages;

@ManyToOne
@JoinColumn(name="pm_id", insertable = false, updatable = false)
private User projectManager;